@model MultiPlug.Ext.ASM.DEK.Models.Apps.ProductFiles.ProductFile

<section class="row-fluid">

    <div class="row-fluid">
        <div class="box">
            <div class="span2">
                <a class="btn btn-black" style="height:44px; width:35px" href="/apps/asm-dek/dek-product-files/"><i class="icon-arrow-left icon-large"></i><p>Back</p></a>
            </div>
            <div class="span8">
                <h3 style="text-align:center">Product File</h3>
            </div>
            <div class="span2">
            </div>
        </div>
    </div>

    <div class="row-fluid">
        <div class="box">
            <table class="table table-striped">
                <thead>
                    <tr>
                        <th>ID</th>
                        <th>Name</th>
                        <th>Data Size</th>
                        <th>Value</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var Parameter in @Model.Parameters)
                    {
                        <tr>
                            <td>@Parameter.Id</td>
                            <td>@Parameter.Name</td>
                            <td>@Parameter.Size</td>
                            <td>@Parameter</td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>

</section>

<script>

</script>